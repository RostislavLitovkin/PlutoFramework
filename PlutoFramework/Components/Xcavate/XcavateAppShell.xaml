<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        xmlns:view="clr-namespace:PlutoFramework.View"
       xmlns:xcavateproperty="clr-namespace:PlutoFramework.Components.XcavateProperty"
       xmlns:xcavate="clr-namespace:PlutoFramework.Components.Xcavate"
             x:Class="PlutoFramework.Components.Xcavate.XcavateAppShell">
    <ShellContent ContentTemplate="{DataTemplate view:MainPage}"
              Route="Account" >
    </ShellContent>
    <ShellContent ContentTemplate="{DataTemplate xcavate:HelpPage}"
           Route="Help" >
    </ShellContent>
    <ShellContent ContentTemplate="{DataTemplate xcavateproperty:XcavatePropertyMarketplacePage}" 
               Route="Marketplace">
    </ShellContent>
</Shell>