<?xml version="1.0" encoding="utf-8" ?>
<Button xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        x:Class="PlutoFramework.Components.Buttons.ElevatedButton"
        BackgroundColor="{StaticResource Primary}"
        TextColor="{StaticResource PrimaryButtonTextColor}"
        BorderColor="{StaticResource PrimaryButtonBorderColor}"
        BorderWidth="{StaticResource PrimaryButtonBorderThickness}"
        FontAttributes="Bold"
        CornerRadius="{StaticResource ButtonCornerRadius}"
        HeightRequest="{StaticResource ButtonHeight}"
        FontFamily="{StaticResource ButtonFontFamily}">
    
</Button>
