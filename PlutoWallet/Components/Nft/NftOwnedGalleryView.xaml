<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PlutoWallet.Components.Nft.NftOwnedGalleryView"
             xmlns:nft="clr-namespace:PlutoWallet.Components.Nft">
    <nft:NftGalleryView PlusIsVisible="True"
                        LoadingIsVisible="{Binding LoadingOwnedNfts}"
                        Nfts="{Binding OwnedNfts}"
                        ErrorIsVisible="False"
                        NoNftsIsVisible="{Binding NoOwnedNfts}"
                        NoNftsText="No owned NFTs"
    />
</ContentView>
